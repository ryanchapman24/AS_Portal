    </div>
</div>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script>
        $(document).ready(function () {
            $('#quality').addClass("active");

            $('.hr-table').DataTable({
                "iDisplayLength": 5,
                "aaSorting": [],
                "aLengthMenu": [[5, 10, 25, 50, -1], [5, 10, 25, 50, "All"]]
            });
        });
    </script>
}
