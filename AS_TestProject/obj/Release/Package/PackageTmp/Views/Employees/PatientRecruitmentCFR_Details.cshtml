                </dt>

                @if (Model.pAQ5 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pAQ5 == 2)
                {
                    <dd>
                        NO
                    </dd>
                }
                else if (Model.pAQ5 == 3)
                {
                    <dd>
                        N/A
                    </dd>
                }

                <dt><strong>Accuracy of Information Rating: </strong><strong style="color: #29AAE3">@Html.DisplayFor(model => model.CFRPerformanceAnalysi.PerformanceRating)</strong></dt>
                <dd></dd>

                <dt>
                    Does the CSR update Browser/Five9 to reflect the proper disposition as needed?
                </dt>

                @if (Model.pAOIQ1 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pAOIQ1 == 2)
                {
                    <dd>
                        NO
                    </dd>
                }
                else if (Model.pAOIQ1 == 3)
                {
                    <dd>
                        N/A
                    </dd>
                }

                <dt>
                    Does the CSR connect to the correct department and select the correct disposition?
                </dt>

                @if (Model.pAOIQ2 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pAOIQ2 == 2)
                {
                    <dd>
                        NO
                    </dd>
                }
                else if (Model.pAOIQ2 == 3)
                {
                    <dd>
                        N/A
                    </dd>
                }

                <dt><strong>Additional Information</strong></dt>
                <dd></dd>

                <dt>
                    Comments
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Comments)
                </dd>

                <dt>
                    Strengths
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Strengths)
                </dd>

                <dt>
                    Action Plan
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.ActionPlan)
                </dd>
            </dl>
        </div>
        <p>
            @Html.ActionLink("Edit", "PatientRecruitmentCFR_Edit", new { id = Model.CFRPatientRecruitmentID }) |
            @Html.ActionLink("Back to Employee Details", "Details", new { id = Model.EmployeeID })
        </p>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script>
        $(document).ready(function () {
            $('#direct').addClass("active");
        });
    </script>
}
