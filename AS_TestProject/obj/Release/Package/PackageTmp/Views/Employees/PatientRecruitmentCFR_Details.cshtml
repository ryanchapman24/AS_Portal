@model AS_TestProject.Entities.CFRPatientRecruitment

@{
    ViewBag.Title = "Details";
}

<div class="col-lg-12 mt">
    <div class="row content-panel" style="padding-left: 20px; padding-right: 20px">
        <h2>Patient Recruitment CFR Details for <span style="color: #29AAE3">@Model.Employee1.FirstName @Model.Employee1.LastName</span></h2>
        <hr />
        <div>
            <dl class="dl-horizontal">
                <dt>
                    Date
                </dt>

                <dd>
                    @Model.DateOfFeedback.ToString("g")
                </dd>

                <dt>
                    Submitted By
                </dt>

                <dd>
                    @Model.Employee.FirstName @Model.Employee.LastName
                </dd>

                <dt>
                    Domain
                </dt>

                <dd>
                    @Model.DomainMaster.FileMask - @Model.DomainMaster.DomainName
                </dd>

                <dt>
                    #Calls
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.C_Calls)
                </dd>

                @*<dt>
                        Today's Conversion Rate
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.ConversionRateToday)
                    </dd>

                    <dt>
                        Week To Date Conversion Rate
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.WeekToDate)
                    </dd>*@

                <dt><strong>Telephone Etiquette Rating: </strong><strong style="color: #29AAE3">@Html.DisplayFor(model => model.CFRPerformanceAnalysi4.PerformanceRating)</strong></dt>
                <dd></dd>

                <dt>
                    Does the CSR ask if the contact name is available with clarity?
                </dt>

                @if (Model.pTEQ1 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pTEQ1 == 2)
                {
                    <dd>
                        NO
                    </dd>
                }
                else if (Model.pTEQ1 == 3)
                {
                    <dd>
                        N/A
                    </dd>
                }

                <dt>
                    Does the CSR transition into the script without hesitation?
                </dt>

                @if (Model.pTEQ2 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pTEQ2 == 2)
                {
                    <dd>
                        NO
                    </dd>
                }
                else if (Model.pTEQ2 == 3)
                {
                    <dd>
                        N/A
                    </dd>
                }

                <dt>
                    Does the CSR display confidence and clarity throughout the call?
                </dt>

                @if (Model.pTEQ3 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pTEQ3 == 2)
                {
                    <dd>
                        NO
                    </dd>
                }
                else if (Model.pTEQ3 == 3)
                {
                    <dd>
                        N/A
                    </dd>
                }

                <dt><strong>Professionalism Rating: </strong><strong style="color: #29AAE3">@Html.DisplayFor(model => model.CFRPerformanceAnalysi3.PerformanceRating)</strong></dt>
                <dd></dd>

                <dt>
                    Does the CSR speak with a delivery rate that is clearly understood?
                </dt>

                @if (Model.pPQ1 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pPQ1 == 2)
                {
                    <dd>
                        NO
                    </dd>
                }
                else if (Model.pPQ1 == 3)
                {
                    <dd>
                        N/A
                    </dd>
                }

                <dt>
                    Does the CSR convey a sense of empathy and have a professional tone?
                </dt>

                @if (Model.pPQ2 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pPQ2 == 2)
                {
                    <dd>
                        NO
                    </dd>
                }
                else if (Model.pPQ2 == 3)
                {
                    <dd>
                        N/A
                    </dd>
                }

                <dt><strong>Compliance Rating: </strong><strong style="color: #29AAE3">@Html.DisplayFor(model => model.CFRPerformanceAnalysi2.PerformanceRating)</strong></dt>
                <dd></dd>

                <dt>
                    Does the CSR identify him/herself with confidence using first and last name?
                </dt>

                @if (Model.pCQ1 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pCQ1 == 2)
                {
                    <dd>
                        NO
                    </dd>
                }
                else if (Model.pCQ1 == 3)
                {
                    <dd>
                        N/A
                    </dd>
                }

                <dt>
                    Does the CSR state that he/she is calling on a recorded line?
                </dt>

                @if (Model.pCQ2 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pCQ2 == 2)
                {
                    <dd>
                        NO
                    </dd>
                }
                else if (Model.pCQ2 == 3)
                {
                    <dd>
                        N/A
                    </dd>
                }

                <dt>
                    Does the CSR follow HIPAA compliance procedures and state the full privacy policy?
                </dt>

                @if (Model.pCQ3 == 1)
                {
                    <dd>
                        YES
                    </dd>
                }
                else if (Model.pCQ3 == 2)
