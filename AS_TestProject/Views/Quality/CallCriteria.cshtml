@{
    Layout = "~/Views/Shared/_RightNavHidden.cshtml";
}


<div class="col-lg-12 mt">
    <div class="row content-panel" style="padding-left: 20px; padding-right: 20px; padding-bottom: 20px">
        <h3 style="color: #29AAE3"><i class="fa fa-check-square-o"></i> Quality: <span style="color: #29AAE3">Call Criteria</span></h3>
        <hr />
        <iframe id="ifm" name="ifm" style="width: 100%; height: 900px" frameborder="0" src="http://app.callcriteria.com/" scrolling="auto"></iframe>
    </div>
</div>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script>
        $(document).ready(function () {
            $('#quality').addClass("active");
        });
    </script>
}