                    <div class="modal fade" id="DAModal" tabindex="-1" role="dialog" aria-labelledby="DAModalLabel" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header" style="margin-bottom: 15px">
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                                    <h4 class="modal-title" id="DAModalLabel">Add New Disciplinary Action for @Model.FirstName @Model.LastName</h4>
                                </div>
                                @using (Html.BeginForm("CreateDA", "Employees", new { }, FormMethod.Post, new { enctype = "multipart/form-data" }))
                                {
                                    @Html.AntiForgeryToken()
                                    <input type="hidden" name="FirstName" value="@Model.FirstName" readonly="readonly" />
                                    <input type="hidden" name="LastName" value="@Model.LastName" readonly="readonly" />
                                    <input type="hidden" name="EmployeeID" value="@Model.EmployeeID" readonly="readonly" />
                                    <div class="modal-body">
                                        <div class="" style="">
                                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })                                           
                                            <div class="form-group">
                                                <div class="col-md-3 control-label">
                                                    Date
                                                </div>
                                                <div class="col-md-9" style="margin-top: -5px">
                                                    <div class="input-group">
                                                        <input id="DAdate" class="form-control datepicker" name="Date" size="16" type="text" value="@ViewBag.Date" required>
                                                        <span class="input-group-btn">
                                                            <button class="btn btn-theme" type="button" style="margin-bottom: -2px"><i class="fa fa-calendar"></i></button>
                                                        </span>
                                                    </div>                                       
                                                </div><br /><br /><br />
                                                
                                                <div class="col-md-3 control-label">
                                                    Reason
                                                </div>
                                                <div class="col-md-9" style="margin-top: -5px">
                                                    <input class="form-control capitalize" name="Reason" type="text" required />
                                                </div><br /><br /><br />

                                                <div class="col-md-3 control-label">
                                                    Explanation
                                                </div>
                                                <div class="col-md-9" style="margin-top: -5px">
                                                    <textarea rows="3" class="form-control capitalize" name="Explanation" required></textarea><br />
                                                    <input type="file" name="files"/>
                                                </div><br /><br /><br /><br />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer" style="margin-top: 20px">
                                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                        <button id="" type="submit" class="btn btn-theme play">Confirm</button>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                    <div class="widget-content">
                        <div class="table-responsive">
                            <table id="datatable-basic-scrolling" class="table table-hover datatable DAtable">
                                <thead>
                                    <tr>
                                        <th style="width: 90%">Date</th>                                   
                                        <th style="width: 200%">Reason</th>
                                        <th>Explanation</th>
                                        <th>File</th>
                                        <th style="width: 80px">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in ViewBag.DisciplinaryActions)
                                    {
                                        <tr>
                                            <td class="text-nowrap">@item.Date.ToString("d")</td>
                                            <td class="text-nowrap" style="color: #29AAE3"><strong>@item.Reason</strong></td>
                                            @if (item.EditByEmployeeID != null)
                                            {
                                                <td>@item.Explanation <i class="fa fa-question-circle tooltips" style="color: #29AAE3; margin-left: 5px" data-original-title="@item.Employee.FirstName @item.Employee.LastName <br> @item.EditTimeStamp.ToString("g")" data-html="true" data-toggle="tooltip" data-placement="top"></i></td>
                                            }
                                            else
                                            {
                                                <td>@item.Explanation</td>
                                            }
                                            @if (item.File != null)
                                            {
                                                <td><a href="@string.Format("{0}{1}","http://192.168.1.8:88/ASPortal",item.File)" target="_blank"><i class="fa fa-paperclip" style="color: #29AAE3; font-size: 20px"></i></a></td>
                                            }
                                            else
                                            {
                                                <td></td>
                                            }
                                            <td>                                     
                                                @if (User.IsInRole("Admin") || User.IsInRole("HR"))
                                                {
                                                    <a href="@Url.Action("EditDA","Employees", new { id = item.DisciplinaryActionID })"><i class="fa fa-edit" style="margin-right: 3px; font-size: 16px"></i> Edit</a><br />
                                                    <a href="@Url.Action("DeleteDA","Employees", new { id = item.DisciplinaryActionID })"><i class="fa fa-trash" style="margin-right: 3px; font-size: 16px"></i> Delete</a>
                                                }
                                            </td>                                                                        
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>


                </div>
            </div>
        </div>
        <p style="padding-bottom: 5px">
            @if (User.IsInRole("Admin") || User.IsInRole("HR"))
            {
                @Html.ActionLink("Edit Employee Information", "Edit", new { id = Model.EmployeeID }) <span>|</span>
            }
            @if (User.IsInRole("Admin") || User.IsInRole("Payroll"))
            {
                <a href="@Url.Action("Index","Payroll", new { id = Model.EmployeeID })"> Manage Time</a> <span>|</span>
            }
            @Html.ActionLink("Back to Directory", "Directory", "Home")
        </p>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script>
        $(document).ready(function () {
            $('#direct').addClass("active");
        });
    </script>
    <script>
        $(document).ready(function () {
            $('.DAtable').DataTable({
                "iDisplayLength": 10,
                "bSort": false,
                "aLengthMenu": [[10, 25, 50, -1], [10, 25, 50, "All"]]
            });
        });
    </script>
}
