@model AS_TestProject.Entities.Employee


<div class="col-lg-12 mt">
    <div class="row content-panel" style="padding-left: 20px; padding-right: 20px">
        <h2>Employee Details: <span style="color: #29AAE3">@Model.FirstName @Model.LastName</span></h2>
        <div class="panel-heading">
            <ul class="nav nav-tabs nav-justified">
                <li class="active">
                    <a data-toggle="tab" href="#information">Information</a>
                </li>
                <li>
                    <a data-toggle="tab" href="#cfrs" class="contact-map">Constructive Feedback Review</a>
                </li>
                <li>
                    <a data-toggle="tab" href="#awards" class="contact-map">Certifications & Awards</a>
                </li>
                <li>
                    <a data-toggle="tab" href="#disciplinaryaction" class="contact-map">Disciplinary Action</a>
                </li>
            </ul>
        </div><!-- --/panel-heading ---->
        <div class="panel-body">
            <div class="tab-content">
                <div id="information" class="tab-pane active">
                    <div>
                        <dl class="dl-horizontal">
                            @if (User.IsInRole("Admin") || User.IsInRole("HR"))
                            {
                                <dt style="width: 190px; margin-left: -20px">
                                    Employee ID
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.EmployeeID)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    First Name
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.FirstName)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Last Name
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.LastName)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Address Line 1
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.AddressLine1)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Address Line 2
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.AddressLine2)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    City
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.City)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    State
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.State)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Zip Code
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.ZipCode)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Email
                                </dt>

                                <dd>
                                    <a href="@string.Format("{0}{1}","mailto:",Model.EmailAddress)">@Model.EmailAddress</a>
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Home Phone
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.HomePhone)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Cell Phone
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.CellPhone)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Birth Date
                                </dt>

                                <dd>
                                    @Model.BirthDate.ToString("M/d/yyyy")
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Hire Date
                                </dt>

                                <dd>
                                    @Model.HireDate.ToString("M/d/yyyy")
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Termination Date
                                </dt>

                                <dd>
                                    @Model.TerminationDate.ToString("M/d/yyyy")
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Rehire Date
                                </dt>

                                if (Model.RehireDate.HasValue)
                                {
                                <dd>
                                    @Model.RehireDate.Value.ToString("M/d/yyyy")
                                </dd>
                                }
                                else
                                {
                                <dd>

                                </dd>
                                }

                                <dt style="width: 190px; margin-left: -20px">
                                    Is this Employee a Manager?
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.IsManager)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Managed by
                                </dt>

                                <dd>
                                    @ViewBag.Manager
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Position
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.Position.PositionName)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Site
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.Site.SiteName)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    File Number
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.FileNumber)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Add Date
                                </dt>

                                <dd>
                                    @Model.AddDate.ToString("M/d/yyyy")
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Added by
                                </dt>

                                <dd>
                                    @ViewBag.Creator
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Is this Employee Active?
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.IsActive)
                                </dd>
                            }
                            else
                            {
                                <dt style="width: 190px; margin-left: -20px">
                                    Employee ID
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.EmployeeID)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    First Name
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.FirstName)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Last Name
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.LastName)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Birth Date
                                </dt>

                                <dd>
                                    @Model.BirthDate.ToString("M/d/yyyy")
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Hire Date
                                </dt>

                                <dd>
                                    @Model.HireDate.ToString("M/d/yyyy")
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Termination Date
                                </dt>

                                <dd>
                                    @Model.TerminationDate.ToString("M/d/yyyy")
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Rehire Date
                                </dt>

                                if (Model.RehireDate.HasValue)
                                {
                                    <dd>
                                        @Model.RehireDate.Value.ToString("M/d/yyyy")
                                    </dd>
                                }
                                else
                                {
                                    <dd>

                                    </dd>
                                }

                                <dt style="width: 190px; margin-left: -20px">
                                    Is this Employee a Manager?
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.IsManager)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Managed by
                                </dt>

                                <dd>
                                    @ViewBag.Manager
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Position
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.Position.PositionName)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Site
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.Site.SiteName)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    File Number
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.FileNumber)
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Add Date
                                </dt>

                                <dd>
                                    @Model.AddDate.ToString("M/d/yyyy")
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Added by
                                </dt>

                                <dd>
                                    @ViewBag.Creator
                                </dd>

                                <dt style="width: 190px; margin-left: -20px">
                                    Is this Employee Active?
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.IsActive)
                                </dd>
                            }
                        </dl>
                    </div>
                </div>


                <div id="cfrs" class="tab-pane">
                    <div class="widget-header">
                        @if (User.IsInRole("Admin") || User.IsInRole("Quality")){<h3 style="margin: 0px"><button class="btn btn-theme btn-md pull-right" data-toggle="modal" data-target="#MortgageCFRModal">+ New Mortgage CFR</button><br /><br /><button class="btn btn-theme btn-md pull-right" data-toggle="modal" data-target="#InsuranceCFRModal" style="margin-top: -8px">+ New Insurance CFR</button><br /></h3>}
                        <h3 style="color: #29AAE3"><i class="fa fa-line-chart"></i> Constructive Feedback Reviews @if (User.IsInRole("Admin") || User.IsInRole("Quality")){<button class="btn btn-theme btn-md pull-right" data-toggle="modal" data-target="#PatientRecruitmentCFRModal" style="margin-top: -10px">+ New Patient Recruitment CFR</button>}</h3>
                    </div>
                    <!-- Mortgage Modal -->
                    <div class="modal fade" id="MortgageCFRModal" tabindex="-1" role="dialog" aria-labelledby="MortgageCFRModalLabel" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header" style="margin-bottom: 15px">
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                                    <h4 class="modal-title" id="MortgageCFRModalLabel">Add New Mortgage CFR for @Model.FirstName @Model.LastName</h4>
                                </div>
                                @using (Html.BeginForm("MortgageCFR_Create", "Employees", new { }, FormMethod.Post, new { enctype = "multipart/form-data" }))
                                {
                                    @Html.AntiForgeryToken()
                                    <input type="hidden" name="EmployeeID" value="@Model.EmployeeID" readonly="readonly" />
                                    <div class="modal-body">
                                        <div class="" style="">
                                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                            <div class="form-group">

                                                <div class="col-md-6 control-label">
                                                    Domain
                                                </div>
                                                <div class="col-md-6" style="margin-top: -5px">
                                                    @Html.DropDownList("DomainMasterID", null, "", htmlAttributes: new { @class = "form-control", required = "true" })
                                                </div><br /><br /><br />

                                                <div class="col-md-9 control-label">
                                                    #Calls
                                                </div>
                                                <div class="col-md-3" style="margin-top: -5px">
                                                    <input class="form-control onlyNum" name="C_Calls" type="text" maxlength="2" required />
                                                </div><br /><br /><br />

                                                <h5 style="color: #29AAE3"><strong>Telephone Etiquette</strong></h5><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR state "Hello" and pause to identify gender?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mTEQ1", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR state the contact name assumptively without asking a question?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mTEQ2", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR state the website of inquiry (if applicable)?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mTEQ3", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR transition into the script without hesitation?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mTEQ4", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR display confidence and clarity throughout the call?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mTEQ5", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                <h5 style="color: #29AAE3"><strong>Professionalism</strong></h5><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR speak with a delivery rate that is clearly understood?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mPQ1", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR convey a sense of empathy and have a professional tone?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mPQ2", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                <h5 style="color: #29AAE3"><strong>Compliance</strong></h5><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR identify him/herself with confidence using first and last name?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mCQ1", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR state that he/she is calling on a recorded line?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mCQ2", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR avoid prohibited references to rates and closing costs?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mCQ3", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                <h5 style="color: #29AAE3"><strong>Adherence</strong></h5><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR ask the questions listed using the word for word script?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mAQ1", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR listen carefully to the answers provided by the contact?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mAQ2", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR ask the verifying questions to determine transfer eligibility?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mAQ3", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR use all 3 steps of the rebuttal process? (T.Phrase, Rebuttal, Close)
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mAQ4", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR exercise good judgement based on the answers to ensure excellent quality?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mAQ5", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                <h5 style="color: #29AAE3"><strong>Accuracy of Information</strong></h5><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR input the LO's NMLS/ITT ID number correctly?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mAOIQ1", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR warm transfer properly to the LO in a professional tone?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mAOIQ2", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR update Browser/Five9 to reflect the proper disposition needed?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mAOIQ3", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                    <div class="col-md-9 control-label">
                                                        Does the CSR connect to the correct department?
                                                    </div>
                                                    <div class="col-md-3" style="margin-top: -5px">
                                                        @Html.DropDownList("mAOIQ4", null, "", htmlAttributes: new { @class = "form-control", required = "true"})
                                                    </div><br /><br /><br />

                                                <h5 style="color: #29AAE3"><strong>Additional Feedback</strong></h5><br />

                                                <div class="col-md-3 control-label">
                                                    Comments
                                                </div>
                                                <div class="col-md-9" style="margin-top: -5px">
                                                    <textarea rows="3" class="form-control capitalize" name="Comments" placeholder="if you know the call IDs pertaining to this review, list them here..." required></textarea>
                                                </div><br /><br /><br /><br /><br />

                                                <div class="col-md-3 control-label">
                                                    Strengths
                                                </div>
                                                <div class="col-md-9" style="margin-top: -5px">
                                                    <textarea rows="3" class="form-control capitalize" name="Strengths" required></textarea>
                                                </div><br /><br /><br /><br /><br />

                                                <div class="col-md-3 control-label">
                                                    Action Plan
                                                </div>
                                                <div class="col-md-9" style="margin-top: -5px">
                                                    <textarea rows="5" class="form-control capitalize" name="ActionPlan" required></textarea>
                                                </div><br /><br /><br /><br /><br />

                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer" style="margin-top: 20px">
                                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                        <button id="" type="submit" class="btn btn-theme play">Confirm</button>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                    <!-- Insurance Modal -->
