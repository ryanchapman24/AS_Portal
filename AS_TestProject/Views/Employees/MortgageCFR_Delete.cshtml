@model AS_TestProject.Entities.CFRMortgage

@{
    ViewBag.Title = "Delete";
}

<div class="col-lg-12 mt">
    <div class="row content-panel" style="padding-left: 20px; padding-right: 20px">
        <h2>Are you sure you want to delete this Mortgage CFR for <span style="color: #29AAE3">@Model.Employee.FirstName @Model.Employee.LastName</span>?</h2>
        <hr />
        <div>
            <dl class="dl-horizontal">
                <dt>
                    Date
                </dt>

                <dd>
                    @Model.DateOfFeedback.ToString("d")
                </dd>

                <dt>
                    Submitted By
                </dt>

                <dd>
                    @Model.Employee1.FirstName @Model.Employee1.LastName
                </dd>

                <dt>
                    Domain
                </dt>

                <dd>
                    @Model.DomainMaster.FileMask - @Model.DomainMaster.DomainName
                </dd>

                <dt>
                    #Calls
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.C_Calls)
                </dd>

                @*<dt>
                        Today's Conversion Rate
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.ConversionRateToday)
                    </dd>

                    <dt>
                        Week To Date Conversion Rate
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.WeekToDate)
                    </dd>*@

